{
    "name": "oneeventgmbh/daily-order-sheet",
    "description": "Display daily WooCommerce orders for Events Calendar events in a printable format",
    "type": "wordpress-plugin",
    "keywords": [
        "wordpress",
        "woocommerce",
        "events-calendar",
        "orders",
        "daily-report",
        "printing"
    ],
    "homepage": "https://oneevent.at",
    "require": {
        "php": ">=7.4",
        "composer/installers": "^1.0 || ^2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "wp-coding-standards/wpcs": "^3.0"
    },
    "authors": [
        {
            "name": "Harry Fesenmayr",
            "homepage": "https://www.fesenmayr.com"
        }
    ],
    "support": {
        "issues": "https://github.com/oneeventgmbh/daily-order-sheet/issues",
        "source": "https://github.com/oneeventgmbh/daily-order-sheet"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "phpcs": "phpcs --standard=WordPress daily-order-sheet.php",
        "phpcbf": "phpcbf --standard=WordPress daily-order-sheet.php"
    }
}
